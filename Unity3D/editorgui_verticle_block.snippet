<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
      <Author>Mars Jang</Author>
      <Title>Vertical Gruop With Block</Title>
      <Description>EditorGUILayout Verticle Group With Block</Description>
      <Shortcut>verticaleditblock</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>typein</ID>
          <Default></Default>
          <ToolTip></ToolTip>
        </Literal>
        <Literal Editable="true">
          <ID>code</ID>
          <Default>code</Default>
          <ToolTip></ToolTip>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[EditorGUILayout.BeginVertical($typein$);
      {
      	$selected$ $code$
      }
      EditorGUILayout.EndVertical();      
      $end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>